{
  "root": false,
  "extends": "//",
  "files": {
    "ignoreUnknown": false
  },
  "linter": {
    "rules": {
      "complexity": {
        "noStaticOnlyClass": "off"
      },
      "style": {
        "noNonNullAssertion": "off",
        "useImportType": "off"
      }
    }
  },
  "javascript": {
    "parser": {
      "unsafeParameterDecoratorsEnabled": true
    }
  }
}
